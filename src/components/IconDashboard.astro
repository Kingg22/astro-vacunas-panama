---
import LayoutDashboard from "./icons/LayoutDashboard.astro";
import Syringe from "./icons/Syringe.astro";
import Calendar from "./icons/Calendar.astro";
import FileText from "./icons/FileText.astro";
import MapPin from "./icons/MapPin.astro";
import Bell from "./icons/Bell.astro";
import Settings from "./icons/Settings.astro";
import Help from "./icons/HelpCircle.astro";
import type { AstroComponentFactory } from "astro/runtime/server/index.js";

const icons: Record<string, AstroComponentFactory> = {
    "layout-dashboard": LayoutDashboard,
    syringe: Syringe,
    calendar: Calendar,
    "file-text": FileText,
    "map-pin": MapPin,
    bell: Bell,
    settings: Settings,
    "help-circle": Help,
  },
  { name } = Astro.props,
  IconComponent = icons[name] || null;
---

{IconComponent ? <IconComponent /> : null}
